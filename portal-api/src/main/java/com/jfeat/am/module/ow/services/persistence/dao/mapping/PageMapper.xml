		<result column="url" property="url" />
		<result column="seq" property="seq" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, org_id AS orgId, org_tag AS orgTag, name, type, url, seq
    </sql>

</mapper>
